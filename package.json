  "author": "William Weiss <william@codification.org>",
  "license": "MIT",
  "private": false,
  "prettier": "@codification/cutwater-build-core/includes/prettier-config",
  "eslintConfig": {
    "extends": [
      "@codification/cutwater"
    ]
  },
  "scripts": {
    "clean": "gulp clean",
