    "mustache": "^3.1.0"
  },
  "devDependencies": {
    "@codification/cutwater-build-aws": "^0.x",
    "@codification/cutwater-build-core": "^0.x",
    "@codification/cutwater-build-web": "^0.x",
    "@types/aws-lambda": "^8.10.36",
